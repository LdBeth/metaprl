#
# External editor
#
if $(SSL_ENABLED)
   PROXYEDIT_FILES[] =
       ../mllib/punix
       ../mllib/setup
       proxyedit_lex
       proxyedit_main

   LIBS += $(LIBMOJAVE_CLIB) $(file $(ROOT)/clib/clib)

   OCAML_LIBS += $(LIBMOJAVE)

   OCamlProgramInstall(.DEFAULT, $(BIN), proxyedit, $(PROXYEDIT_FILES))

clean:
   $(CLEAN) proxyedit_lex.ml
