#
# External editor
#
if $(SSL_ENABLED)
   # This application is really only useful in native code
   NATIVE_ENABLED = true
   BYTE_ENABLED = false

   PROXYEDIT_FILES =\
       ../mllib/setup\
       proxyedit_lex\
       proxyedit_main

   LIBS += $(LIB)/liblmcutil $(LIB)/clib

   OCAML_LIBS += $(LIB)/lm

   OCamlProgramInstall(.DEFAULT, $(BIN), proxyedit, $(PROXYEDIT_FILES))

clean:
   $(CLEAN) proxyedit_lex.ml
