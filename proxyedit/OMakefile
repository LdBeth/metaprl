#
# External editor
#
if $(SSL_ENABLED)
   PROXYEDIT_FILES[] =
       ../mllib/punix
       ../mllib/setup
       proxyedit_lex
       proxyedit_main

   LIBS[] +=
       $(LIB)/liblmcutil
       $(LIB)/clib

   OCAML_LIBS += $(LIB)/lm

   OCamlProgramInstall(.DEFAULT $(MPINSTALL), $(BIN), proxyedit, $(PROXYEDIT_FILES))

clean:
   $(CLEAN) proxyedit_lex.ml
