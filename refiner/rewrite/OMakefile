CAMLP4INCLUDES = -I../refsig

OCAMLINCLUDES =\
	$(OCAMLINCLUDES)\
	-I $(ROOT)/libmojave/stdlib\
	-I $(ROOT)/libmojave/util\
	-I ../../mllib\
	-I ../refbase\
	-I ../refsig\
	-I ../term_std\
	-I ../term_gen\
	-I ../term_ds

