(*
 * Common types for proofs.
 *)

open Refiner.Refiner.Refine

open Tactic_type

(*
 * This is the info needed to construct a tactic_arg.
 * Should be similar to Tactic_type.tactic_argument (which
 * is hidden.
 *)
type tactic_argument =
   { ref_label : string;
     ref_args : term attributes;
     ref_fcache : cache
   }

