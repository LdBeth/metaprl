# C preprocessing
OCAMLFLAGS = $(OCAMLFLAGS) -pp "$(CAMLP4MACRO)"

OCAMLINCLUDES +=\
	-I $(ROOT)/libmojave/stdlib\
	-I $(ROOT)/libmojave/util\
	-I $(ROOT)/libmojave/unix

include Files

FILES = $(MLLIB_FILES)

OCamlLibraryInstall($(MPINSTALL), $(LIB), util, $(FILES))

#
# Clean up
#
clean:
    $(CLEAN)
