#
# Don't need any libraries.
#
OCAMLINCLUDES += ../display
OCAMLDEPFLAGS += -notopval

# .mlz files
MLZFILES[] =
   	session_sig
	shell_sig
	shell_internal_sig
   	shell_syscall_sig
	shell_p4_sig
	browser_sig
	shell_theory

# MetaPRL Files
MPFILES[] =
	proof_edit
	mptop
	shell_command
	shell_package
	shell_root
	shell_fs
	shell_current
	shell_syscall
	shell_state
	shell
	browser_resource
	shell_theory

# All library files
FILES[] =
	shell_util
	browser_syscall
	session_sig
	session_io
	session_current
	session
	shell_sig
  	browser_sig
	browser_state
	browser_copy
	browser_edit
	shell_tex
	proof_edit
	package_info
