	shell_browser
	shell_theory

PRLFiles($(MPFILES))
OCamlLibrary(theory, $(FILES))

OCamlGeneratedFiles(browser_copy.ml browser_session_io.ml session_io.ml)

#
# Clean up
#
clean:
    $(CLEAN) browser_copy.ml browser_session_io.ml session_io.ml

all: theory$(LIB_SUFFIX)

