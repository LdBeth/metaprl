#
# Don't need any libraries.
#
OCAMLINCLUDES += -I . -I ../display

include Files

Theory($(MPFILES))

#
# Copy the files in inputs
#
LIBINPUTS = $(file $(addprefix $(LIB)/, $(INPUTS)))

$(LIBINPUTS): $(LIB)/%: inputs/%
	$(LN) $(in $(LIB), $(CWD))/$< $@

$(MPINSTALL): $(LIBINPUTS)

#
# Clean up
#
clean:
    $(CLEAN)

all: theory$(LIB_SUFFIX)
