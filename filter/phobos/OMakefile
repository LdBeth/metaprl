#
# Standard includes
#
OCAMLINCLUDES[] +=
	../base

#
# Type files
#
MLZFILES =\
	phobos_parser

#
# Library files
#
include Files

OCamlGeneratedFiles(phobos_lexer.ml  phobos_parser.ml)

OCamlLibraryInstall($(MPINSTALL), $(LIB), filterphobos, $(PHOBOS_FILES))

#
# Clean up
#
clean:
    $(CLEAN) phobos_lexer.ml

all: filterphobos$(LIB_SUFFIX)
