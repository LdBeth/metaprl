#
# Includes
#
OCAMLINCLUDES[] +=
   $(CAMLP4LIB)
   $(ROOT)/libmojave/stdlib
   $(ROOT)/libmojave/util
   $(ROOT)/mllib
   $(ROOT)/refiner/refbase
   $(ROOT)/refiner/refsig
   $(ROOT)/refiner/refiner
   $(ROOT)/refiner/reflib
   $(ROOT)/$(ENSEMBLE_DIR)

#
# Basic camlp4 preprocessing
#
OCAMLPPFLAGS = -pp "camlp4o$(EXE) -I $(CAMLP4LIB) q_MLast.cmo"

#
# Type files
#
MLZFILES =\
	tactic_boot_sig

#
# Library files
#
include Files

OCamlLibraryInstall($(MPINSTALL), $(LIB), prooflib, $(PROOF_FILES))

clean:
    $(CLEAN)

all: filterlib$(LIB_SUFFIX)
