#
# Includes
#
OCAMLINCLUDES =\
	$(OCAMLINCLUDES)\
   -I $(CAMLP4LIB)\
   -I $(ROOT)/libmojave/stdlib\
   -I $(ROOT)/libmojave/util\
   -I $(ROOT)/mllib\
   -I $(ROOT)/refiner/refbase\
   -I $(ROOT)/refiner/refsig\
   -I $(ROOT)/refiner/refiner\
   -I $(ROOT)/refiner/reflib\
	-I $(ROOT)/$(ENSEMBLE_DIR)\

#
# Basic camlp4 preprocessing
#
OCAMLPPFLAGS = -pp "camlp4o$(EXE) -I $(CAMLP4LIB) q_MLast.cmo"

#
# Type files
#
MLZFILES =\
	tactic_boot_sig

#
# Library files
#
include Files

OCamlLibraryInstall($(MPINSTALL), $(LIB), prooflib, $(PROOF_FILES))

#
# Clean up
#
clean:
    $(CLEAN)

all: filterlib$(LIB_SUFFIX)
